<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.yawjalik.steinsboard">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/icon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/icon_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".FarisActivity">
        </activity>
        <activity android:name=".Credits">
        </activity>
        <activity android:name=".LukakoActivity">
        </activity>
        <activity
                android:name=".SuzuhaActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".MoekaActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".KurisuActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".Splash"
                android:screenOrientation="portrait"
                android:theme="@style/splashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".DaruActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".MayushiiActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".OkarinActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity
                android:name=".MainActivity"
                android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>